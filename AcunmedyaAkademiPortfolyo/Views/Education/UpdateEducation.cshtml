@model AcunmedyaAkademiPortfolyo.Models.TblEducation
@{
    ViewBag.Title = "UpdateEducation";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Eğitimi Güncelleme Sayfası</h2>
<form method="post">
    <label>Eğitim Id</label>
    @Html.TextBoxFor(x => x.Educationld, new { @class = "form-control" })
    <br />
    <label>Eğitim Adı</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    <label>Eğitim Alt Başlığı</label>
    @Html.TextBoxFor(x => x.SubTitle, new { @class = "form-control" })
    <br />
    <label>Eğitim Zamanı</label>
    @Html.TextBoxFor(x => x.Period, new { @class = "form-control" })
    <br />
    <label>Eğitim Açıklaması</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Eğitimi Güncelle</button>
</form>

